<% include ../partials/header.ejs %>


<main class="container">
	<h1>Analysis Result for <%= tempSurvey.name %></h1>
	<form method="get">
		<ul class="list-group">
            <li class="list-group-item ">
                <fieldset class="form-group" >
                    <legend>General Information:</legend>
                    <label for="title">Number of respondents: <%= tempSurveyAnsArry.length %></label><br />
                    <label>Number of Two Option Questions: <%= tempSurvey.twoOption.length %></label><br />
                    <label>Number of Multiple Option Questions: <%= tempSurvey.multipleChoice.length %></label><br />
                    <label>Number of Short Questions: <%= tempSurvey.shortAnswer.length %></label>
                </fieldset>
            </li>
            <li class="list-group-item">
                <fieldset>
                    <legend>Question Information:</legend>
                    <ul class="list-group">
                        <li>
                            <h5>Two Option</h5>
                            <% for (var i = 0; i < tempSurvey.twoOption.length; i++) { %>
                            <label>Question :<%= (i+1) %><%=tempSurvey.twoOption[i].twOptionQue %></label><br />
                                <!-- <label >Option1:<%= tempSurvey.twoOption[i].option1 %></label> 
                                    <label >Option2:<%= tempSurvey.twoOption[i].option2 %></label> -->
                                <div class="progress">
                                  <div class="progress-bar" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= opt1Count/tempSurveyAnsArry.length*100 %>%;">
                                    Option1: <%= tempSurvey.twoOption[i].option1 %> <%= opt1Count/tempSurveyAnsArry.length*100 %>%
                                  </div>
                                  <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: <%= (tempSurveyAnsArry.length-opt1Count)/tempSurveyAnsArry.length*100 %>%;">
                                    Option2: <%= tempSurvey.twoOption[i].option2 %> <%= (tempSurveyAnsArry.length-opt1Count)/tempSurveyAnsArry.length*100 %>%
                                  </div>
                                </div>                                
                                <!-- <label >Percent:<%= (tempSurveyAnsArry.length-opt1Count)/tempSurveyAnsArry.length %></label> <br />    -->                 
                                <% } %>
                        </li>
                        <li>
                            <h5>Multiple Choice</h5>
                            <% for (var i = 0; i < tempSurvey.multipleChoice.length; i++) { %>                               
                            <label>Question :<%= (i+tempSurvey.twoOption.length+1) %><%=tempSurvey.multipleChoice[i].mulQue%></label><br />
                                <% for (var k = 0; k < tempSurvey.multipleChoice[i].mulOpt.length; k++) { %>
                                <label>Option<%= k +1 %>:<%= tempSurvey.multipleChoice[i].mulOpt[k] %></label><label>Percent:TBD<%= %></label> <br />  
                                <% } %>
                            <% } %>
                        </li>
                        <li>
                            <h5>Short Answer</h5>
                            <% for (var i = 0; i < tempSurvey.shortAnswer.length; i++) { %>
                            <label>Question :<%= (i+tempSurvey.twoOption.length+tempSurvey.multipleChoice.length+1) %><%=tempSurvey.shortAnswer[i] %></label><br />
                                <% for (var k = 0; k < tempSurveyAnsArry.length; k++) { %>
                                 <label>Answer<%=k+1 %>:<%=tempSurveyAnsArry[k].shortAnswer[i].shrtAns %></label><br />
                                <% } %>
                            <% } %>
                        </li>
                    </ul>

                </fieldset>
            </li>




</ul>
	</form>
    <div class="text-center">
    	<a href="javascript:window.print()" class="btn btn-info btn-lg" id="print-button">
    		<span class="glyphicon glyphicon-print"></span> Print 
        </a>
    </div>

	
</main>

<% include ../partials/boot.ejs %>
<% include ../partials/footer.ejs %>