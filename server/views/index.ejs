<%- include partials/header.ejs %>
   

<main class="container">
	<h1>Survey List</h1>
	<ul class="list-group">
		<% for (var i = 0; i < surveys.length; i++) { %>
		 <li class="list-group-item">
		 	<a href="/takeSurvey/<%= surveys[i]._id %>" title="Take Survey">
		 		<%= surveys[i].name %>
			</a>
		 	<br />
		 </li>
		<% } %>
	</ul>
</main>


